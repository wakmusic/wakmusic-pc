    "react-hook-form": "^7.45.0",
    "react-is": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.13.0",
    "recoil": "^0.7.7",
    "rollup-plugin-license": "^3.0.1",
    "styled-components": "^6.0.0-rc.3",
    "swiper": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "vite": "^4.3.9",
