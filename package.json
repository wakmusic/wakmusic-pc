{
  "name": "wakmusic-pc",
  "version": "1.1.0",
  "description": "WAKTAVERSE MUSIC Desktop App",
  "repository": "https://github.com/wakmusic/wakmusic-pc.git",
  "author": "wakmusic",
  "main": ".vite/build/electron.js",
  "scripts": {
    "dev": "electron-forge start",
    "package": "electron-forge package \"$@\"",
    "make": "electron-forge make \"$@\"",
    "publish": "electron-forge publish \"$@\"",
    "dev:web": "vite --port 3000",
    "build:web": "vite build -c vite.renderer.config.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "eslint src --ext .ts,.tsx --fix && prettier --write \"src/**/*.{ts,tsx}\"",
    "check:types": "tsc --noEmit",
    "check:eslint": "yarn lint"
  },
  "dependencies": {
    "@electron/remote": "^2.0.11",
    "@xhayper/discord-rpc": "^1.0.24"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.3.0",
    "@electron-forge/maker-appx": "^6.4.1",
    "@electron-forge/maker-dmg": "^6.4.1",
    "@electron-forge/maker-zip": "^6.4.1",
    "@electron-forge/plugin-vite": "^6.3.0",
    "@electron-forge/publisher-github": "^6.4.1",
    "@electron/osx-sign": "^1.0.5",
    "@tanstack/react-virtual": "^3.0.0-alpha.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/gtag.js": "^0.0.14",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/youtube": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "axios": "^1.4.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "electron": "^25.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^10.13.1",
    "lodash.throttle": "^4.1.1",
    "lottie-web": "^5.12.2",
    "overlayscrollbars": "^2.2.1",
    "overlayscrollbars-react": "^0.5.1",
